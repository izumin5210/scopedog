"./spec/scopedog/record_class_spec.rb[1:2:1]": SELECT "users".* FROM "users" WHERE
  "users"."deleted" = 0
"./spec/scopedog/record_class_spec.rb[1:3:1:1:1]": SELECT "users".* FROM "users" WHERE
  "users"."deleted" = 0 AND "users"."registered" = 1
"./spec/scopedog/record_class_spec.rb[1:3:2:1:1]": SELECT "users".* FROM "users" WHERE
  (users."deleted" IS NULL OR users."deleted" != 0)
"./spec/scopedog/record_class_spec.rb[1:3:2:2:1]": SELECT "users".* FROM "users" WHERE
  "users"."deleted" = 0 AND "users"."deleted" = 0
"./spec/scopedog/record_class_spec.rb[1:3:2:3:1]": SELECT "users".* FROM "users"
